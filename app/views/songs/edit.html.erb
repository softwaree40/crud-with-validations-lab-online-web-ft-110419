<%= form_for @song do |f| %>
<%= f.label 'Song Title' %><br>
<%= f.text_field :title %><br>
<%= f.label 'Song Released' %><br>
<%= f.text_area :released %><br>
<%= f.label 'Song Release Year' %><br>
<%= f.text_field :release_year %><br>
<%= f.label 'Song Artist Name' %><br>
<%= f.text_field :artist_name %><br>
<%= f.label 'Song Genre' %><br>
<%= f.text_field :genre %><br>
 <%= f.submit "Submit Song" %>
<% end %>
