<h1>Edit Information</h1>
<%= form_for (@song) do |f| %>
<%= f.label 'Title' %><br>
<%= f.text_field :title %><br>
<%= f.label 'Song Released' %><br>
<%= f.check_box :released %><br>
<%= f.label 'Release Year' %><br>
<%= f.text_field :release_year %><br>
<%= f.label 'Artist Name' %><br>
<%= f.text_field :artist_name %><br>
<%= f.label 'Genre' %><br>
<%= f.text_field :genre %><br><br>
<%= f.submit %>

<% end %>
